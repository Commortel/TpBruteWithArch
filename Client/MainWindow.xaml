<Window x:Class="Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="600" Width="800">
    <Grid>
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="250,200,0,0" Name="TextLogin" Text="Login" VerticalAlignment="Top" Width="100" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="250,250,0,0" Name="TextPassword" Text="Password" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="350,200,0,0" Name="textBox1"  VerticalAlignment="Top" Width="120" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="350,250,0,0" Name="textBox2" VerticalAlignment="Top" Width="120" />
        <Button Content="Submit" Height="23" HorizontalAlignment="Left" Margin="350,300,0,0" Name="Submit" VerticalAlignment="Top" Width="120" />
        <Button Content="NewAccount" Height="23" HorizontalAlignment="Left" Margin="350,350,0,0" Name="NewAccount" VerticalAlignment="Top" Width="120" />
    </Grid>
</Window>
